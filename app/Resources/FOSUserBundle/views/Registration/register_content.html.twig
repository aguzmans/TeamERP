{% block contentTitle %}
    <div><h3>Register administrator user</h3></div>
{% endblock %}
<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="form-horizontal">
{% include 'FOSUserBundle::user_form.html.twig' %}    
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
        <input class="btn btn-primary" type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" />
        <input class="btn btn-default" type="reset" value="Cancel" />
    </div>
  </div>
</form>

